[checking]
recursionlevel=2

# Set number of concurrently running checks (default 10).
#
# High values may reduce overall run time, but increase the risk of
# running into rate limits.
threads=1

[filtering]
# Check external links (i.e. ones not within the domain given as a
# command-line argument).
checkextern=1

# Don't fail on links that result in HTTP 301 or 302, rather than 200.
ignorewarnings=http-redirected
